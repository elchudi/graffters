#get markers ids
curl http://localhost:5000/markers

#get single marker
curl http://localhost:5000/markers/marker_id

#delete marker
curl http://localhost:5000/markers/marker_id -X DELETE -v

#create new image in marker. If marker does not exist, it creats it
curl http://localhost:5000/markers/marker_id -d "base64=DATOS" -X PUT -v

#get images ids
curl http://localhost:5000/images

#get single image
curl http://localhost:5000/images/image_id

#delete image
curl http://localhost:5000/images/image_id -X DELETE -v

#create or update image with image_id
curl http://localhost:5000/images/image_id -d "base64=DATOS" -X PUT -v
